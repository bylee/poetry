<?xml version="1.0" encoding="UTF-8"?>
<Scene xmlns="http://mobello.github.com/tau/scene" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	viewPortWidth="320px" viewPortHeight="480px">
	<ScrollPanel id="panel1">
		<ImageView id="imagePanel" src="/image/image2-1.jpeg"><ImageView.styles width="320px"></ImageView.styles></ImageView>
		<Panel>
				<Button>
					<Button.styles display="-webkit-box" borderStyle="none"
						WebkitBorderRadius="0px"></Button.styles>
					<Button.label normal="Good"></Button.label>
				</Button>
				<Button>
					<Button.styles width="48px" backgroundImage="url('/image/addcomment.gif')"
						borderStyle="none" backgroundColor="#FFFFFF" color="transparent"
						display="-webkit-box" backgroundRepeat="no-repeat"
						WebkitBorderRadius="0px"></Button.styles>

				</Button>



				<Panel>
					<Panel.styles WebkitBoxFlex="4" width="180px"
						backgroundColor="#FFFFFF"></Panel.styles>
				</Panel>
				<Button>
					<Button.styles display="-webkit-box" borderStyle="none"
						WebkitBorderRadius="0px"></Button.styles>
					<Button.label normal="G 27"></Button.label>
				</Button>
				<Button>
					<Button.styles borderStyle="none"
						backgroundImage="url('/image/comments.gif')" backgroundColor="#FFFFFF"
						color="#516691" borderColor="transparent" display="-webkit-box"
						WebkitBorderRadius="0px" backgroundPosition="left center" width="78px"
						textAlign="right" backgroundSize="50%"></Button.styles>
					<Button.label normal="19"></Button.label>
				</Button>
				<Panel.styles width="100%" position="" display="-webkit-box"
					WebkitBoxOrient="Horizontal" WebkitBoxPack="center" height="32px"></Panel.styles>
			</Panel>
	</ScrollPanel>
